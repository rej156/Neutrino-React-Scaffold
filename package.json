{
  "version": "1.0.0",
  "private": true,
  "description": "Get started with React!",
  "engines": {
    "node": ">=6.9"
  },
  "keywords": [
    "neutrino",
    "react",
    "starter",
    "webpack"
  ],
  "config": {
    "presets": [
      "neutrino-preset-react",
      "neutrino-custom.js"
    ],
    "neutrino": {
      "entry": {
        "vendor": [
          "react",
          "react-dom",
          "react-dom", 
          "react-hammerjs",
          "react-helmet",
          "react-hot-loader",
          "react-redux",
          "react-router",
          "react-router-dom",
          "react-router-redux",
          "redux"
        ]
      }
    }
  },
  "scripts": {
    "start": "neutrino start",
    "build": "neutrino build"
  },
  "author": "Eric Juta <ericjohnjuta@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-stage-0": "^6.22.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-smsenesac": "^2.3.0",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-class-property": "^1.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "neutrino": "latest",
    "neutrino-preset-airbnb-base": "latest",
    "neutrino-preset-jest": "^4.3.0",
    "neutrino-preset-react": "latest",
    "react-addons-test-utils": "^15.4.2"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "ramda": "0.21.0",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-hammerjs": "0.5.0",
    "react-helmet": "2.3.1",
    "react-hot-loader": "next",
    "react-redux": "5.0.2",
    "react-router": "4.0.0-beta.6",
    "react-router-dom": "4.0.0-beta.6",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-api-middleware": "1.0.2",
    "redux-async-queue": "1.0.0",
    "redux-thunk": "2.2.0",
    "styled-jsx": "^0.5.7",
    "superagent": "2.0.0",
    "superagent-cache": "1.4.1",
    "ua-parser-js": "0.7.11",
    "webpack": "^2.2.1"
  }
}
